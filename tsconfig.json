{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "webworker"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "sourceMap": true,
    "noEmit": true,
    "downlevelIteration": true,
    "jsx": "react",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@ducks/*": [
        "src/ducks/*"
      ],
      "~/*": [
        "src/util/*"
      ],
      "#/*": [
        "static/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}